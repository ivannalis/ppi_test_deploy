                                <p style="font-size: 14px;font-weight: bold;">"<%= projectdetail.judul_project %>"</p>
                                <h4>Deskripsi Project</h4>
                                <%- projectdetail.deskripsi_project %>
                            </div>


                            <div class="single-section exprerience">
                                <h4>Project Impact</h4>
                                <div class="container">
                                    <div class="row">
                                        <div class="col-lg-4 col-md-4 col-12">
                                            <div class="process-item">
                                                <i class="lni lni-protection" style="font-size: 60px"></i>
                                                <h4 style="margin-top: 5px;margin-bottom:0px;color:#2042e3;">Proteksi
                                                </h4>
                                                <p
                                                    style="color:#081828; font-size: 22px;font-weight: bold;margin-top:0px !important;margin-bottom:0px">
