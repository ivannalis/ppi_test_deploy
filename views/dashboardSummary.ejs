    }

    table td,
    table thead th {
        white-space: normal !important;
        padding: 10px 10px !important;
        font-size: 14px;
    }

    #scrollSidebar {
        height: 500px;
        position: relative;
        overflow-y: scroll;
    }

    /* width */
    ::-webkit-scrollbar {
        width: 8px;
    }

    /* Track */
    ::-webkit-scrollbar-track {
        background: #f1f1f1;
    }

    /* Handle */
    ::-webkit-scrollbar-thumb {
        background: #888;
    }

    /* Handle on hover */
    ::-webkit-scrollbar-thumb:hover {
        background: #555;
    }
</style>

